                 }
 
 #elif defined(TARGET_GENTOO)
@@ -391,7 +391,7 @@ int main(int argc, char **argv) {
                                         NULL)) < 0) {
 
                         if (r != -ENOENT)
-                                log_warning("Failed to read /etc/sysconfig/i18n: %s", strerror(-r));
